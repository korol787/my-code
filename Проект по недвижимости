       "2        3      8  2015  \n",
       "4        1      6  2018  \n",
       "5        0      9  2018  "
      ]
     },
     "execution_count": 52,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "df_for_price = df.loc[:, ['last_price', 'price_for_square', 'rooms', 'floor_group', 'city_centre_nearest', 'weekday', 'month', 'year']]\n",
    "df_for_price['last_price'] = df_for_price['last_price']/1000000\n",
    "def floor_reverse(row):\n",
    "    floor=row['floor_group']\n",
    "    if floor == 'первый':\n",
    "        return 0\n",
    "    if floor == 'последний':\n",
    "        return 1\n",
    "    return 2\n",
    "\n",
    "df_for_price['floor_group'] = df.apply(floor_reverse, axis=1)\n",
    "\n",
    "df_for_price.head()"
   ]
  },
